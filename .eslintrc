{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "shared-node-browser": true,
    "amd": true,
    "jquery": true,
    "es6": true
  },
  "extends": [
    "standard"
  ],
  "rules": {
    "no-mixed-operators": "off",
    "generator-star-spacing": "off",
    "indent": [
      "error",
      2
    ]
  },
  "globals": {
    "__dirname": "readonly",
    "CustomException": "readonly"
  }
}
